<%= form_for @cp_press_run, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :cprun_name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :cprun_name, :class => 'text_field' %>
    </div>
  </div>

  <%= render :partial => 'dl_press_run', :locals => {:f => f} %>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                cp_press_runs_path, :class => 'btn' %>
  </div>
<% end %>


